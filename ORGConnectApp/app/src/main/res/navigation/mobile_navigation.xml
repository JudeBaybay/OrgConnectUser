<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_event">



    <fragment android:id="@+id/navigation_event"
        android:name="com.example.orgconnectapp.ui.event.EventFragment"
        android:label="Newsfeed"
        tools:layout="@layout/fragment_event"/>

    <fragment android:id="@+id/navigation_faculty"
