        android:label="Faculty"
        tools:layout="@layout/fragment_faculty"/>

    <fragment android:id="@+id/navigation_menu"
        android:name="com.example.orgconnectapp.ui.menu.MenuFragment"
        android:label="Clubs N' Orgs"
        tools:layout="@layout/fragment_menu"/>

    <fragment android:id="@+id/navigation_gallery"
